BindingArrayTraits :: struct {
	dims_count: u32,
	dims:       [MAX_ARRAY_DIMS]u32,
}

ArrayTraits :: struct {
	dims_count:           u32,
	// Each entry is either:
	// - specialization constant dimension
	// - OpTypeRuntimeArray
	// - the array length otherwise
